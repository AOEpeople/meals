{
    "name": "aoepeople/meals",
    "license": "MIT",
    "type": "project",
    "description": "",
    "autoload": {
        "psr-4": {
            "Mealz\\": "src/Mealz/",
            "Application\\Migrations\\": "app/DoctrineMigrations/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "ext-calendar": "*",
        "ext-json": "*",
        "creatissimo/mattermost-bundle": "1.1.12",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "friendsofsymfony/oauth-server-bundle": "^1.5",
        "friendsofsymfony/rest-bundle": "^2.4",
        "hwi/oauth-bundle": "^0.6.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/serializer-bundle": "^2.4",
        "ocramius/proxy-manager": "^2.0",
        "paypal/paypal-checkout-sdk": "^1.0.0",
        "php-http/httplug-bundle": "^1.11",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "stof/doctrine-extensions-bundle": "~1.2.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/symfony": "^3.4",
        "tecnickcom/tcpdf": "6.2.26",
        "twig/extensions": "^1.0",
        "twig/twig": "^1.41",
        "whiteoctober/tcpdf-bundle": "dev-master"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "laminas/laminas-zendframework-bridge": "^1.3",
        "leaphub/phpcs-symfony2-standard": "~2.0.0",
        "phploc/phploc": "~2.0",
        "phpmd/phpmd": "~2.2",
        "phpunit/phpunit": "^7.5",
        "rector/rector": "^0.11.40",
        "sebastian/phpcpd": "~2.0",
        "squizlabs/php_codesniffer": "2.4.0",
        "zendframework/zend-code": "^3.4"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
