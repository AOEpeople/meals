    "ext-pdo": "*",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.19",
    "friendsofphp/proxy-manager-lts": "^1.0.2",
    "hwi/oauth-bundle": "^2.2.0",
    "jms/serializer-bundle": "^5.4.0",
    "lcobucci/jwt": "^4.1",
