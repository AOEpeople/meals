  "conflict": {
    "symfony/symfony": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "tests": [
      "mkdir -p build/artifacts/qa/",
      "vendor/bin/phpunit -d memory_limit=-1 -c tests/phpunit.xml.dist"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "php-http/discovery": true,
      "symfony/runtime": true
    }
  }
