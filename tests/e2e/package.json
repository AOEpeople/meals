{
  "config": {
    "staging": "https://meals-staging.aoe.com/",
    "local": "http://meals.test:8033/"
  },
  "scripts": {
    "preopen": "yarn",
    "open": "cypress open",
    "open:staging": "cross-env-shell cypress open --env baseUrl=$npm_package_config_staging",
    "open:local": "cross-env-shell cypress open --env baseUrl=$npm_package_config_local",
    "run": "cypress run",
    "run:staging": "cross-env-shell cypress run --headless --browser chrome --env baseUrl=$npm_package_config_staging --spec '**/meals.aoe.com/**/*'",
    "run:local": "cross-env-shell cypress run --headless --browser chrome --env baseUrl=$npm_package_config_local --spec '**/meals.aoe.com/**/*'",
    "ts:check": "tsc --noEmit",
    "format": "prettier",
    "lint-staged": "lint-staged",
    "reports:clean": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "reports:combine": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "reports:generate": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports --inline"
  },
  "devDependencies": {
    "@types/node": "^16.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.2.0",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "lint-staged": "^11.1.2",
    "mocha": "^9.0.3",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0"
  },
  "dependencies": {}
}
